<h1>New Place</h1>

<%= form_for @place do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :image_url %>
  <%= f.text_field :image_url %>

  <%= f.label :rating %>
  <%= f.number_field :rating %>

  <%= f.submit "Create" %>
<% end %>
